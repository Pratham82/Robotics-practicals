import ch.aplu.robotsim.*;
class Movewithoutgear
{
  Movewithoutgear()
  {
    TurtleRobot robot = new TurtleRobot();
    robot.forward(100);
    robot.left(50);
    robot.forward(100);
    robot.right(50);
    robot.forward(100);
    robot.exit();
  }
  public static void main(String[]args)
  {
    new Movewithoutgear();
  }
}            
                                               
